{
  "license": "MIT",
  "name": "eleventy-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/clenemt/eleventy-webpack"
  },
  "scripts": {
    "clean": "rimraf _site",
    "format": "prettier --write **/*.{js,s?css,yml} !_site",
    "start": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start:webpack": "webpack --watch",
    "start:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
    "build:webpack": "webpack",
    "build:eleventy": "eleventy",
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "date-fns": "^2.16.1",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "markdown-it": "^12.0.2",
    "markdown-it-anchor": "^6.0.0",
    "markdown-it-attrs": "^3.0.3",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.6",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.5",
    "source-map-explorer": "^2.5.0",
    "svg-sprite": "^1.5.0",
    "vinyl": "^2.2.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-manifest-plugin": "^3.0.0-rc.0"
  },
  "dependencies": {}
}
